{
  "compilerOptions": {
    "jsx": "react-jsx",
    "module": "commonjs",
    "noImplicitAny": true,
    "target": "ES5",
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": { 
      "@/*": ["src/*"], 
      "hooks": ["src/hooks"]
    }
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
